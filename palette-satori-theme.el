;;; Commentary:

;; Palette theme generated from Gruvbox Dark colors

;;; Code:
(eval-when-compile
 (require 'palette-theme-lib))

(deftheme palette-satori "Palette theme generated from Gruvbox Dark colors")

(palette-theme-gen
 'palette-satori
 ((bg . "#3f3f4c")
  (bg-bold . "#525263")
  (fg . "#edeff2")
  (fg-bold . "#cccaca")
  (red . "#ac6a76")
  (red-bold . "#c47987")
  (green . "#7b8c58")
  (green-bold . "#90a366")
  (yellow . "#bc945a")
  (yellow-bold . "#d4a765")
  (blue . "#58698c")
  (blue-bold . "#7086b2")
  (purple . "#7b5e8d")
  (purple-bold . "#9572ab")
  (aqua . "#82a1b2")
  (aqua-bold . "#95b9cc")
  )
 :soft
 'purple-bold
 )

(provide-theme 'palette-satori)

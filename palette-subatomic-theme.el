;;; Commentary:

;; Palette theme generated from subatomic colors

;;; Code:
(eval-when-compile
 (require 'palette-theme-lib))

(deftheme palette-subatomic "Palette theme generated from subatomic colors")

(palette-theme-gen
 'palette-subatomic
 (
  (bg . "#2e3043")
  (bg-bold . "#696e92")
  (fg . "#e5e5e5")
  (fg-bold . "#cebca5")
  (red . "#ea8673")
  (red-bold . "#ff2222")
  (green . "#81a257")
  (green-bold . "#22aa22")
  (yellow . "#f9b529")
  (yellow-bold . "#ffd700")
  (blue . "#8aa6bc")
  (blue-bold . "#9dbbd3")
  (purple . "#7b5e8d")
  (purple-bold . "#9c71a5")
  (aqua . "#82a1b2")
  (aqua-bold . "#95b9cc")
  (aqua        . "#689d6a")
  (aqua-bold   . "#8ec07c")
  )
 :normal
 purple
 )

(provide-theme 'palette-subatomic)
